# Python program to print all positive numbers in a list

# First example
list1 = [12, -7, 5, 64, -14]
print("Input:", "list1 =", list1)

print("Output:", end=" ")
for num in list1:
    if num > 0:
        print(num, end=", ")
        
print("\n")

# Second example
list2 = [12, 14, -95, 3]
print("Input:", "list2 =", list2)

positive_numbers = []
for num in list2:
    if num > 0:
        positive_numbers.append(num)

print("Output:", positive_numbers)
